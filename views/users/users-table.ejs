<!DOCTYPE html>
<%- include('../template/header', {title:title}) %>
<body>
    <%- include('../nav/nav', {title: title}) %>
    <div class="container">
        <div class="row row-title p-2">
            <div class="col-6">               

            </div>
            <div class="col-6">
                <h1 class="h1 text-end"><%= title %></h1>
            </div>
        </div>
        <div class="card p-4"> 
            <table class="table table-hover table-responsive">
                <thead>
                    <tr>
                        <th>Nombre</th>
                        <th>Status</th>
                        <th>Rol</th>
                        <th>Acciones</th>                     
                    </tr>
                </thead>
                <tbody>
                    <% users.forEach (user =>{ %>
                        <tr>
                            <td><%= user.userFirstName %></td>
                            <td><%= user.userLastName %></td>
                            <td><%= user.userDepartment %></td>
                            <td><button>ver</button></td>       
                        </tr>
                    <% }) %>                  
                </tbody>                
            </table>
        </div>
    </div>  
</body>
</html>